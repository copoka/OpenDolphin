##----------------------------------------------------------------------------- start MMLBody
  <MmlBody>

##-------------------------------------------------------------------------------------------------------------------------------
## PatientModule
##-------------------------------------------------------------------------------------------------------------------------------
    <MmlModuleItem>
        <docInfo contentModuleType="patientInfo">
            <securityLevel>
                <accessRight permit="all">
                    <mmlSc:facility>
                        <mmlSc:facilityName mmlSc:facilityCode="creator">記載者施設</mmlSc:facilityName>
                    </mmlSc:facility>
                </accessRight>
            </securityLevel>
            <title generationPurpose="record">${patientHelper.getGenerationPurpose()}</title>
                <docId>
                    <uid>${patientHelper.getDocId()}</uid>
                </docId>
                <confirmDate>${patientHelper.getConfirmDate()}</confirmDate>
                <mmlCi:CreatorInfo>
                    <mmlPsi:PersonalizedInfo>
                        <mmlCm:Id mmlCm:type="local" mmlCm:tableId="MML0024">${patientHelper.getCreatorId()}</mmlCm:Id>
                        <mmlPsi:personName>
                            <mmlNm:Name mmlNm:repCode="I" mmlNm:tableId="MML0025">
                                <mmlNm:fullname>${patientHelper.getCreatorName()}</mmlNm:fullname>
                            </mmlNm:Name>
                        </mmlPsi:personName>
                    </mmlPsi:PersonalizedInfo>
                </mmlCi:CreatorInfo>
                <extRefs />
            </docInfo>
            <content>
                <mmlPi:PatientModule>
                    <mmlPi:uniqueInfo>
                        <mmlPi:masterId>
                            <mmlCm:Id mmlCm:type="facility" mmlCm:tableId="MML0024">${patientHelper.getPatientId()}</mmlCm:Id>
                        </mmlPi:masterId>
                    </mmlPi:uniqueInfo>
                    <mmlPi:personName>
                        <mmlNm:Name mmlNm:repCode="I" mmlNm:tableId="MML0025">
                            <mmlNm:family>$!{patientHelper.getPatientFamily()}</mmlNm:family>
                        </mmlNm:Name>
                        <mmlNm:Name mmlNm:repCode="I" mmlNm:tableId="MML0025">
                            <mmlNm:given>$!{patientHelper.getPatientGiven()}</mmlNm:given>
                        </mmlNm:Name>
                        <mmlNm:Name mmlNm:repCode="I" mmlNm:tableId="MML0025">
                            <mmlNm:fullname>$!{patientHelper.getPatientName()}</mmlNm:fullname>
                        </mmlNm:Name>
                        <mmlNm:Name mmlNm:repCode="P" mmlNm:tableId="MML0025">
                            <mmlNm:family>$!{patientHelper.getPatientKanaFamily()}</mmlNm:family>
                        </mmlNm:Name>
                        <mmlNm:Name mmlNm:repCode="P" mmlNm:tableId="MML0025">
                            <mmlNm:given>$!{patientHelper.getPatientKanaGiven()}</mmlNm:given>
                        </mmlNm:Name>
                        <mmlNm:Name mmlNm:repCode="P" mmlNm:tableId="MML0025">
                            <mmlNm:fullname>$!{patientHelper.getPatientKanaName()}</mmlNm:fullname>
                        </mmlNm:Name>
                    </mmlPi:personName>
                    <mmlPi:birthday>$!{patientHelper.getPatientBirthday()}</mmlPi:birthday>
                    <mmlPi:sex>$!{patientHelper.getPatientGender()}</mmlPi:sex>
                    <mmlPi:addresses>
                        <mmlAd:Address>
                            <mmlAd:full repCode="I">$!{patientHelper.getPatientAddress()}</mmlAd:full>
                            <mmlAd:zip>$!{patientHelper.getPatientZip()}</mmlAd:zip>
                        </mmlAd:Address>
                    </mmlPi:addresses>
                    <mmlPi:phones>
                        <mmlPh:Phone>
                            <mmlPh:area></mmlPh:area>
                            <mmlPh:city></mmlPh:city>
                            <mmlPh:number></mmlPh:number>
                            <mmlPh:memo>$!{patientHelper.getPatientTelephone()}</mmlPh:memo>
                        </mmlPh:Phone>
                     </mmlPi:phones>
                </mmlPi:PatientModule>
            </content>
       </MmlModuleItem>
##-------------------------------------------------------------------------------------------------------------------------------
## HealthInsurance
##-------------------------------------------------------------------------------------------------------------------------------
#if ($patientHelper.getInsurances())
#foreach ($pvtIns in $patientHelper.getInsurances())
            <MmlModuleItem>
                <docInfo contentModuleType="healthInsurance">
                    <securityLevel>
                        <accessRight permit="all">
                            <mmlSc:facility>
                                <mmlSc:facilityName mmlSc:facilityCode="creator">記載者施設</mmlSc:facilityName>
                            </mmlSc:facility>
                        </accessRight>
                    </securityLevel>
                    <title generationPurpose="record">${patientHelper.getGenerationPurpose()}</title>
                    <docId>
                        <uid>${patientHelper.getDocId()}</uid>
                    </docId>
                    <confirmDate>${patientHelper.getConfirmDate()}</confirmDate>
                    <mmlCi:CreatorInfo>
                        <mmlPsi:PersonalizedInfo>
                            <mmlCm:Id mmlCm:type="local" mmlCm:tableId="MML0024">${patientHelper.getCreatorId()}</mmlCm:Id>
                            <mmlPsi:personName>
                                <mmlNm:Name mmlNm:repCode="I" mmlNm:tableId="MML0025">
                                    <mmlNm:fullname>${patientHelper.getCreatorName()}</mmlNm:fullname>
                                </mmlNm:Name>
                            </mmlPsi:personName>
                        </mmlPsi:PersonalizedInfo>
                    </mmlCi:CreatorInfo>
                    <extRefs />
                </docInfo>
                <content>
                    <mmlHi:HealthInsuranceModule mmlHi:countryType="JPN">
#if ($pvtIns.getInsuranceClass() && $pvtIns.getInsuranceClassCode())
                        <mmlHi:insuranceClass mmlHi:ClassCode="${pvtIns.getInsuranceClassCode()}" mmlHi:tableId="MML0031">${pvtIns.getInsuranceClass()}</mmlHi:insuranceClass>
#end
			<mmlHi:insuranceNumber>${pvtIns.getInsuranceNumber()}</mmlHi:insuranceNumber>
                    	<mmlHi:clientId>
                            <mmlHi:group>${pvtIns.getClientGroup()}</mmlHi:group>
                            <mmlHi:number>${pvtIns.getClientNumber()}</mmlHi:number>
                    	</mmlHi:clientId>
                    	<mmlHi:familyClass>${pvtIns.getFamilyClass()}</mmlHi:familyClass>
                    	<mmlHi:startDate>${pvtIns.getStartDate()}</mmlHi:startDate>
                    	<mmlHi:expiredDate>${pvtIns.getExpiredDate()}</mmlHi:expiredDate>
#if ($pvtIns.getPayInRatio())
                    	<mmlHi:paymentInRatio>${pvtIns.getPayInRatio()}</mmlHi:paymentInRatio>
#end
#if ($pvtIns.getPayOutRatio())
                    	<mmlHi:paymentOutRatio>${pvtIns.getPayOutRatio()}</mmlHi:paymentOutRatio>
#end
#if ($pvtIns.getPVTPublicInsuranceItem())
                        <mmlHi:publicInsurance>
#foreach ($publicItem in $pvtIns.getPVTPublicInsuranceItem())
                            <mmlHi:publicInsuranceItem mmlHi:priority="${publicItem.getPriority()}">
                                <mmlHi:providerName>${publicItem.getProviderName()}</mmlHi:providerName>
				<mmlHi:provider>${publicItem.getProvider()}</mmlHi:provider>
				<mmlHi:recipient>${publicItem.getRecipient()}</mmlHi:recipient>
				<mmlHi:startDate>${publicItem.getStartDate()}</mmlHi:startDate>
				<mmlHi:expiredDate>${publicItem.getExpiredDate()}</mmlHi:expiredDate>
				<mmlHi:paymentRatio mmlHi:ratioType="${publicItem.getPaymentRatioType()}">${publicItem.getPaymentRatio()}</mmlHi:paymentRatio>
                            </mmlHi:publicInsuranceItem>
#end
                        </mmlHi:publicInsurance>
#end
                    </mmlHi:HealthInsuranceModule>
                </content>
            </MmlModuleItem>
#end
#end
##-------------------------------------------------------------------------------------------------------------------------------
## progressCourse
##-------------------------------------------------------------------------------------------------------------------------------
    <MmlModuleItem>
        <docInfo contentModuleType="progressCourse">
            <securityLevel>
#foreach ($ar in $mmlHelper.getAccessRights())        
                <accessRight permit="${ar.getPermission()}">
#if ($ar.getLicenseeCodeType() == "facilityCode")          
                    <mmlSc:facility>
                        <mmlSc:facilityName mmlSc:facilityCode="${ar.getLicenseeCode()}">${ar.getLicenseeName()}</mmlSc:facilityName> 
                    </mmlSc:facility>
#elseif ($ar.getLicenseeCodeType() == "personCode")
                    <mmlSc:person>
                        <mmlSc:personName mmlSc:personCode="${ar.getLicenseeCode()}">${ar.getLicenseeName()}</mmlSc:personName> 
                    </mmlSc:person>
#end
                </accessRight>
#end          
            </securityLevel>
            <title generationPurpose="${mmlHelper.getPurpose()}">${mmlHelper.getTitle()}</title> 
            <docId>
                <uid>${mmlHelper.getDocId()}</uid> 
#if ($mmlHelper.getParentId())
                <parentId relation="${mmlHelper.getParentIdRelation()}">${mmlHelper.getParentId()}</parentId>
#end  
                <groupId groupClass="record">${mmlHelper.getGroupId()}</groupId> 
            </docId>
            <confirmDate firstConfirmDate="${mmlHelper.getFirstConfirmDate()}">${mmlHelper.getConfirmDate()}</confirmDate> 
            <mmlCi:CreatorInfo>
                <mmlPsi:PersonalizedInfo>
                    <mmlCm:Id mmlCm:type="${mmlHelper.getCNCreatorIdType()}" mmlCm:tableId="${mmlHelper.getCNCreatorIdTableId()}">${mmlHelper.getCNCreatorId()}</mmlCm:Id> 
                    <mmlPsi:personName>
                        <mmlNm:Name mmlNm:repCode="I" mmlNm:tableId="MML0025">
                            <mmlNm:fullname>${mmlHelper.getCreatorName()}</mmlNm:fullname> 
                        </mmlNm:Name>
                    </mmlPsi:personName>
                    <mmlFc:Facility>
                        <mmlFc:name mmlFc:repCode="I" mmlFc:tableId="MML0025">${mmlHelper.getCNFacilityName()}</mmlFc:name> 
                        <mmlCm:Id mmlCm:type="${mmlHelper.getCNFacilityIdType()}" mmlCm:tableId="${mmlHelper.getCNFacilityIdTableId()}">${mmlHelper.getCNFacilityId()}</mmlCm:Id> 
                    </mmlFc:Facility>
                </mmlPsi:PersonalizedInfo>
                <mmlCi:creatorLicense mmlCi:tableId="MML0026">${mmlHelper.getCreatorLicense()}</mmlCi:creatorLicense> 
            </mmlCi:CreatorInfo>
#if ($mmlHelper.getSchema())
            <extRefs>
#foreach($schema in $mmlHelper.getSchema())
#set ($extRef = $schema.getExtRefModel())
                <mmlCm:extRef mmlCm:contentType="${extRef.getContentType()}" mmlCm:medicalRole="${extRef.getMedicalRole()}" mmlCm:title="${extRef.getTitle()}" mmlCm:href="${extRef.getHref()}"/>
#end
            </extRefs>
#else		
            <extRefs />
#end        
        </docInfo>
        <content>
            <mmlPc:ProgressCourseModule>
                <mmlPc:FreeExpression>
                    ${mmlHelper.getFreeExpression()}
                </mmlPc:FreeExpression>
            </mmlPc:ProgressCourseModule>
        </content>
    </MmlModuleItem>
##-----------------------------------------------------------------------------------------------------------------------------------
## Claim
##-----------------------------------------------------------------------------------------------------------------------------------
#if ($mmlHelper.getClaimBundle())
        <MmlModuleItem>
            <docInfo contentModuleType="claim">
                <securityLevel>
                    <accessRight permit="all">
                        <mmlSc:facility>
                            <mmlSc:facilityName mmlSc:facilityCode="creator">記載者施設</mmlSc:facilityName>
                        </mmlSc:facility>
                    </accessRight>
                </securityLevel>
                <title generationPurpose="record">${mmlHelper.getGenerationPurpose()}</title>
                <docId>
                    <uid>${mmlHelper.getClaimDocId()}</uid>
                    <groupId groupClass="record">${mmlHelper.getGroupId()}</groupId>
                </docId>
                <confirmDate>${mmlHelper.getConfirmDate()}</confirmDate>
                <mmlCi:CreatorInfo>
                    <mmlPsi:PersonalizedInfo>
                        <mmlCm:Id mmlCm:type="local" mmlCm:tableId="MML0024">${mmlHelper.getCreatorId()}</mmlCm:Id>
                        <mmlPsi:personName>
                            <mmlNm:Name mmlNm:repCode="I" mmlNm:tableId="MML0025">
                                <mmlNm:fullname>${mmlHelper.getCreatorName()}</mmlNm:fullname>
                            </mmlNm:Name>
                        </mmlPsi:personName>
                        <mmlFc:Facility>
                            <mmlFc:name mmlFc:repCode="I" mmlFc:tableId="MML0025">${mmlHelper.getFacilityName()}</mmlFc:name>
                            <mmlCm:Id mmlCm:type="insurance" mmlCm:tableId="MML0027">${mmlHelper.getJmariCode()}</mmlCm:Id>
                        </mmlFc:Facility>
                        <mmlDp:Department>
                            <mmlDp:name mmlDp:repCode="I" mmlDp:tableId="MML0025">${mmlHelper.getCreatorDeptDesc()}</mmlDp:name>
                            <mmlCm:Id mmlCm:type="medical" mmlCm:tableId="MML0029">${mmlHelper.getCreatorDept()}</mmlCm:Id>
                        </mmlDp:Department>
                    </mmlPsi:PersonalizedInfo>
                    <mmlCi:creatorLicense mmlCi:tableId="MML0026">${mmlHelper.getCreatorLicense()}</mmlCi:creatorLicense>
                </mmlCi:CreatorInfo>
                <extRefs />
            </docInfo>
            <content>
                <claim:ClaimModule>
                    <claim:information claim:status="perform"
                                       claim:orderTime="${mmlHelper.getConfirmDate()}"
                                       claim:admitFlag="false"
#if ($mmlHelper.getHealthInsuranceGUID())
                                       claim:insuranceUid="${mmlHelper.getHealthInsuranceGUID()}"
#end   
                                       claim:defaultTableId="dolphin_2001_10_03">
                        <mmlHi:insuranceClass mmlHi:ClassCode="${mmlHelper.getHealthInsuranceClassCode()}" mmlHi:tableId="MML0031">${mmlHelper.getHealthInsuranceDesc()}</mmlHi:insuranceClass>
                    </claim:information>
##-------------------------------------
## start bundle
##-------------------------------------
#foreach ($bundle in $mmlHelper.getClaimBundle())
#if ($bundle.getClassCode())
                    <claim:bundle claim:classCode="${bundle.getClassCode()}" claim:classCodeId="Claim007">
                        <claim:className>$!{bundle.getClassName()}</claim:className>
#else
                    <claim:bundle>
#end
                        <claim:bundleNumber>${bundle.getBundleNumber()}</claim:bundleNumber>
##-------------------------------------
## start item
##-------------------------------------
#foreach ($item in $bundle.getClaimItem())
#if ($item.getClassCode()=="0")
                        <claim:item claim:subclassCode="${item.getClassCode()}" claim:subclassCodeId="Claim003" claim:code="${item.getCode()}">
#else
                        <claim:item claim:subclassCodeId="Claim003" claim:code="${item.getCode()}">
#end
                            <claim:name>${item.getName()}</claim:name>
#if ($item.getNumber())
                            <claim:number claim:numberCode="10" claim:numberCodeId="Claim004">${item.getNumber()}</claim:number>
#end
                        </claim:item>
#if ($item.getCanDispose())
                       <claim:item claim:subclassCodeId="Claim003" claim:code="099309901">
                            <claim:name>(残量廃棄)</claim:name>
                       </claim:item>
#end
#end
##-------------------------------------
## admin To item 2010-03-09
##-------------------------------------
#if ($bundle.getAdmin())
                        <claim:administration claim:adminCode="${bundle.getAdminCode()}">${bundle.getAdmin()}</claim:administration>
#end
##------------------------------------- end item
#if ($bundle.getMemo())	 
                        <claim:memo>${bundle.getMemo()}</claim:memo>
#end	    
                    </claim:bundle>
#end
##--------------------------------------end bundle
                </claim:ClaimModule>
            </content>
        </MmlModuleItem>
#end
  </MmlBody>
</Mml>