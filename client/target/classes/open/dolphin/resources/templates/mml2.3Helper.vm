    </mmlCi:CreatorInfo>
    <masterId>
      <mmlCm:Id mmlCm:type="${mmlHelper.getCNPatientIdType()}" mmlCm:tableId="${mmlHelper.getCNPatientIdTableId()}">${mmlHelper.getCNPatientId()}</mmlCm:Id> 
    </masterId>
    <toc>
      <tocItem>http://www.medxml.net/MML/SharedComponent/Common/1.0</tocItem> 
